      <_ATGShaderSymbols Include="$(ProjectDir)Src/Shaders/Compiled/*.pdb" Exclude="$(ProjectDir)Src/Shaders/Compiled/*Xbox*.pdb" />
    </ItemGroup>
    <Delete Files="@(_ATGShaderHeaders)" />
    <Delete Files="@(_ATGShaderSymbols)" />
  </Target>
